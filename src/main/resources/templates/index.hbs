    <!-- ***** Header Area Start ***** -->
    <header class="header-area header-sticky wow slideInDown" data-wow-duration="0.75s" data-wow-delay="0s">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav">
                        <!-- ***** Logo Start ***** -->
                        <a href="/" class="logo"><img src="images/perroygato.png" style="width: 60px; "> GESOC
                        </a>

                        <!-- ***** Logo End ***** -->
                        <!-- ***** Menu Start ***** -->
                        <ul class="nav">
                            <li><a href="/" class="active">Principal</a></li>
                            <li><a href="/adopcion/0">Adopciones</a></li>
                            <li><a href="#">Mascotas Perdidas</a></li>
                            {{#if persona}}
                            <li>
                                <div class="main-white-button">
                                        <a href="/panel">{{persona.nombre}}</a>
                                </div>
                            </li>
                            {{else}}
                                {{#if asociacion}}
                                    <li>
                                        <div class="main-white-button">
                                            <a href="/panel">{{asociacion.nombre}}</a>
                                        </div>
                                    </li>
                                {{else}}
                                        <li>
                                            <div class="main-white-button" data-bs-toggle="modal" data-bs-target="#exampleModal">
                                                <a href="#"><i class="fa fa-plus"></i>Ingresar</a>
                                            </div>
                                        </li>
                                {{/if}}
                            {{/if}}
                        </ul>
                        <a class='menu-trigger'>
                            <span>Menu</span>
                        </a>
