    <div class="main-banner">
        <!-- Modal -->
        <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">Espera un momento amigo!</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                        <form method="post" action="/login">
                        <div class="modal-body">

                            <div class="mb-3">
                                <label for="usuario" class="form-label">Usuario</label>
                                <input type="text" class="form-control" name="usuario" id="usuario"/>
                                <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
                            </div>
                            <div class="mb-3">
                                <label for="password" class="form-label">Password</label>
                                <input type="password" class="form-control" name= "password" id="password"/>
                            </div>
                            <div class="mb-3 form-check">
                                <input type="checkbox" class="form-check-input" id="exampleCheck1">
                                <label class="form-check-label" for="exampleCheck1">Check me out</label>
                            </div>
                            <button class="btn btn-primary" role="button" type="submit" aria-disabled="true">Ingresar</button>
                            <button class="btn btn-danger" data-bs-dismiss="modal">Cerrar</button>

                        </div>
                        <div class="modal-footer">
                            <a href="/crearCuenta" class="btn btn-success" role="button" type="submit" aria-disabled="true">Crear Cuenta</a>
                        </div>
                    </form>

                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="top-text header-text">
                        <h6>Adopta cerca de tu zona</h6>
                        <h2>Encontra a tu futuro mejor amigo</h2>
                    </div>
                </div>
                <div class="col-lg-12">
                    <form id="search-form" name="gs" method="submit" role="search" action="#">
                        <div class="row">
                            <div class="col-lg-3 align-self-center">
                                <fieldset>
                                    <select name="area" class="form-select" aria-label="Area" id="chooseCategory" onchange="this.form.click()">
                                        <option selected>Zona</option>
                                        <option value="Villa Urquiza">Villa Urquiza</option>
                                        <option value="Flores">Flores</option>
                                        <option value="Palermo">Palermo</option>
                                    </select>
                                </fieldset>
                            </div>
                            <div class="col-lg-3 align-self-center">
                                <fieldset>
                                    <select name="area" class="form-select" aria-label="Area" id="chooseCategory" onchange="this.form.click()">
